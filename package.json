{
  "name": "project-manager-next",
  "displayName": "Project Manager Next",
  "description": "Easily manage multiple projects and branches",
  "version": "0.1.0",
  "publisher": "Dolov",
  "engines": {
    "vscode": "^1.79.1"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "project-manager.refreshCurrent",
          "when": "view == current"
        },
        {
          "command": "project-manager.refreshFavorite",
          "when": "view == favorite"
        }
      ],
      "projectManager.sideBar.current.title": [
        
      ],
      "view/item/context": [
        {
          "command": "project-manager.openInNew",
          "when": "view == current || view == favorite",
          "group": "inline"
        },
        {
          "command": "project-manager.copy",
          "when": "view == current || view == favorite",
          "group": "inline"
        },
        {
          "command": "project-manager.favorite",
          "when": "viewItem == favorite",
          "group": "inline"
        },
        {
          "command": "project-manager.favorited",
          "when": "viewItem == favorited",
          "group": "inline"
        }
      ]
    },
    "commands": [{
      "command": "project-manager.helloWorld",
      "title": "Hello World"
    }, {
      "command": "project-manager.openInNew",
      "title": "在新窗口中打开",
      "icon": "images/open-new.svg"
    }, {
      "command": "project-manager.copy",
      "title": "复制",
      "icon": "images/copy.svg"
    }, {
      "command": "project-manager.favorite",
      "title": "收藏",
      "icon": "images/favorite.svg"
    }, {
      "command": "project-manager.favorited",
      "title": "取消收藏",
      "icon": "images/favorited.svg"
    }, {
      "command": "project-manager.refreshCurrent",
      "title": "刷新",
      "icon": "images/refresh.svg"
    }, {
      "command": "project-manager.refreshFavorite",
      "title": "刷新",
      "icon": "images/refresh.svg"
    }
  ],
    "viewsContainers": {
      "activitybar": [{
        "id": "projectManager",
        "title": "项目管理器",
        "icon": "images/project.svg"
      }]
    },
    "views": {
      "projectManager": [{
          "id": "current",
          "name": "最近使用"
        },
        {
          "id": "favorite",
          "name": "收藏夹"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dolov/vscode-project-manager"
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "1.79.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}